<h1> Registration </h1>

<%= form_for :user, url: '/users' do |f| %>
  <p>First Name:</p><p> <%= f.text_field :first_name %></p>
  <p>Last Name: </p><p><%= f.text_field :last_name %></p>
  <p>Email: </p><p><%= f.text_field :email %></p>
  <p>Password: </p><p><%= f.password_field :password %></p>
  <p>Password Confirmation: </p><p><%= f.password_field :password_confirmation %></p>
  <p><%= f.submit "Register!" %></p>
<% end %>